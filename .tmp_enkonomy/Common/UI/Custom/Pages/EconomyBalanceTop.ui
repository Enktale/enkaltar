$C = "../Common.ui";

$C.@PageOverlay {}

$C.@Container {
  Anchor: (Width: 520, Height: 360);

  #Content {
    LayoutMode: Top;
    Padding: (Full: 18);

    Label #TitleText {
      Style: (
        FontSize: 18,
        RenderBold: true,
        TextColor: #ffffff,
        HorizontalAlignment: Center
      );
      Anchor: (Height: 28);
      Padding: (Bottom: 10);
    }

    Label #Line0 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line1 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line2 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line3 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line4 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line5 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line6 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line7 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line8 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
    Label #Line9 { Style: (FontSize: 12, TextColor: #bfcdd5); Padding: (Bottom: 4); }
  }
}

